  <xsd:complexType name="XdTokenListType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en-us">A data type for a white space-delimited list of normalized token values. 
        This type provides efficient storage and transmission of multiple normalized token values in a single field.
        Useful for identifiers, codes, and other scenarios where multiple related normalized token values need to be
        transmitted together. Token values have leading and trailing whitespace removed and internal whitespace
        normalized to single spaces.</xsd:documentation>
      <xsd:appinfo>
        <rdf:Description rdf:about="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#XdTokenListType">
          <rdfs:isDefinedBy rdf:resource="https://semanticdatacharter.com/docs/sdc4/sdc4_xsd_Complex_Type_sdc4_XdTokenListType.html#XdTokenListType"/>
          <rdfs:subClassOf rdf:resource="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#XdAnyType"/>
          <rdfs:label xml:lang="en-us">XdTokenListType</rdfs:label>
          <rdf:type rdf:resource="https://semanticdatacharter.com/ns/sdc4/sdc4/CMC"/>          
          <rdfs:comment xml:lang="en-us">A data type for a white space-delimited list of normalized token values. This type provides efficient storage and transmission of multiple normalized token values in a single field.</rdfs:comment>
        </rdf:Description>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="sdc4:XdAnyType">
        <xsd:sequence>
          <xsd:element maxOccurs="1" minOccurs="0" name="xdtokenlist-value" type="sdc4:TokenListSimpleType">
            <xsd:annotation>
              <xsd:documentation xml:lang="en-us">Space-delimited list of normalized token values. Values should be valid tokens separated by single spaces. Token values have leading and trailing whitespace removed and internal whitespace normalized to single spaces.</xsd:documentation>
              <xsd:appinfo>
                <rdf:Description rdf:about="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#XdTokenListType/xdtokenlist-value">
                  <rdfs:domain rdf:resource="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#XdTokenListType"/>
                  <rdfs:range rdf:resource="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#TokenListSimpleType"/>
                  <rdfs:label xml:lang="en-us">xdtokenlist-value</rdfs:label>                  
                  <rdf:type rdf:resource="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#TokenListSimpleType"/>                  
                  <rdfs:comment xml:lang="en-us">Space-delimited list of normalized token values. Values should be valid tokens separated by single spaces. Token values have leading and trailing whitespace removed and internal whitespace normalized to single spaces.</rdfs:comment>
                </rdf:Description>
              </xsd:appinfo>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

