      <xsd:appinfo>
        <rdf:Description rdf:about="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#Cluster">
          <rdfs:subClassOf rdf:resource="https://semanticdatacharter.com/ns/sdc4/sdc4/CMS"/>
