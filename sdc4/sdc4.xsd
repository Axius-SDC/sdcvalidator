                  <rdfs:range rdf:resource="https://semanticdatacharter.com/ns/sdc4/sdc4.xsd#PositiveIntegerListSimpleType"/>
                  <rdfs:label xml:lang="en-us">xdpositiveintegerlist-value</rdfs:label>                  
